{
  "dashboard": {
    "id": null,
    "uid": "zerostate-execution",
    "title": "ZeroState - Execution Performance",
    "tags": ["zerostate", "execution", "wasm", "guild", "performance"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "10s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "annotations": {
      "list": [
        {
          "datasource": "Prometheus",
          "enable": true,
          "expr": "ALERTS{alertname=~\".*Execution.*\"}",
          "iconColor": "orange",
          "name": "Execution Alerts"
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "Task Execution Success Rate",
        "type": "gauge",
        "gridPos": {"x": 0, "y": 0, "w": 6, "h": 6},
        "targets": [
          {
            "expr": "sum(rate(execution_tasks_total{result=\"success\"}[5m])) / sum(rate(execution_tasks_total[5m])) * 100",
            "refId": "A"
          }
        ],
        "options": {
          "showThresholdLabels": true,
          "showThresholdMarkers": true
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 90, "color": "yellow"},
                {"value": 99, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Active Guilds",
        "type": "stat",
        "gridPos": {"x": 6, "y": 0, "w": 6, "h": 3},
        "targets": [
          {
            "expr": "execution_guilds_total{state=\"active\"}",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "blue"},
                {"value": 5, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Tasks/sec",
        "type": "stat",
        "gridPos": {"x": 12, "y": 0, "w": 6, "h": 3},
        "targets": [
          {
            "expr": "rate(execution_tasks_total[1m])",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "decimals": 2,
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 4,
        "title": "Total Receipts Generated",
        "type": "stat",
        "gridPos": {"x": 18, "y": 0, "w": 6, "h": 3},
        "targets": [
          {
            "expr": "execution_receipts_total",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 5,
        "title": "Guild Members",
        "type": "stat",
        "gridPos": {"x": 6, "y": 3, "w": 6, "h": 3},
        "targets": [
          {
            "expr": "sum(execution_guild_members)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "id": 6,
        "title": "Avg Task Duration (p50/p95/p99)",
        "type": "stat",
        "gridPos": {"x": 12, "y": 3, "w": 12, "h": 3},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(execution_task_duration_seconds_bucket[5m]))",
            "refId": "A",
            "legendFormat": "p50"
          },
          {
            "expr": "histogram_quantile(0.95, rate(execution_task_duration_seconds_bucket[5m]))",
            "refId": "B",
            "legendFormat": "p95"
          },
          {
            "expr": "histogram_quantile(0.99, rate(execution_task_duration_seconds_bucket[5m]))",
            "refId": "C",
            "legendFormat": "p99"
          }
        ],
        "options": {
          "orientation": "horizontal",
          "textMode": "value_and_name"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "decimals": 3,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 10, "color": "yellow"},
                {"value": 25, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 7,
        "title": "Task Execution Duration Distribution",
        "type": "timeseries",
        "gridPos": {"x": 0, "y": 6, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(execution_task_duration_seconds_bucket[1m]))",
            "refId": "A",
            "legendFormat": "p50"
          },
          {
            "expr": "histogram_quantile(0.95, rate(execution_task_duration_seconds_bucket[1m]))",
            "refId": "B",
            "legendFormat": "p95"
          },
          {
            "expr": "histogram_quantile(0.99, rate(execution_task_duration_seconds_bucket[1m]))",
            "refId": "C",
            "legendFormat": "p99"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 15
            },
            "unit": "s"
          }
        }
      },
      {
        "id": 8,
        "title": "WASM Memory Usage",
        "type": "timeseries",
        "gridPos": {"x": 12, "y": 6, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(execution_memory_bytes_bucket[1m]))",
            "refId": "A",
            "legendFormat": "p50"
          },
          {
            "expr": "histogram_quantile(0.95, rate(execution_memory_bytes_bucket[1m]))",
            "refId": "B",
            "legendFormat": "p95"
          },
          {
            "expr": "histogram_quantile(0.99, rate(execution_memory_bytes_bucket[1m]))",
            "refId": "C",
            "legendFormat": "p99"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "fillOpacity": 20
            },
            "unit": "bytes",
            "max": 134217728,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 104857600, "color": "yellow"},
                {"value": 125829120, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 9,
        "title": "Task Results (Success/Failure)",
        "type": "timeseries",
        "gridPos": {"x": 0, "y": 14, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(execution_tasks_total{result=\"success\"}[1m])",
            "refId": "A",
            "legendFormat": "Success"
          },
          {
            "expr": "rate(execution_tasks_total{result=\"failure\"}[1m])",
            "refId": "B",
            "legendFormat": "Failure"
          },
          {
            "expr": "rate(execution_tasks_total{result=\"timeout\"}[1m])",
            "refId": "C",
            "legendFormat": "Timeout"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "fillOpacity": 30,
              "stacking": {"mode": "normal"}
            },
            "unit": "tasks/s"
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Success"},
              "properties": [{"id": "color", "value": {"fixedColor": "green", "mode": "fixed"}}]
            },
            {
              "matcher": {"id": "byName", "options": "Failure"},
              "properties": [{"id": "color", "value": {"fixedColor": "red", "mode": "fixed"}}]
            },
            {
              "matcher": {"id": "byName", "options": "Timeout"},
              "properties": [{"id": "color", "value": {"fixedColor": "orange", "mode": "fixed"}}]
            }
          ]
        }
      },
      {
        "id": 10,
        "title": "Guild Lifecycle",
        "type": "timeseries",
        "gridPos": {"x": 12, "y": 14, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(execution_guilds_total{state=\"created\"}[1m])",
            "refId": "A",
            "legendFormat": "Created"
          },
          {
            "expr": "rate(execution_guilds_total{state=\"dissolved\"}[1m])",
            "refId": "B",
            "legendFormat": "Dissolved"
          },
          {
            "expr": "execution_guilds_total{state=\"active\"}",
            "refId": "C",
            "legendFormat": "Active"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "fillOpacity": 20
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 11,
        "title": "Task Cost Distribution",
        "type": "timeseries",
        "gridPos": {"x": 0, "y": 22, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(execution_task_cost_bucket[1m]))",
            "refId": "A",
            "legendFormat": "p50"
          },
          {
            "expr": "histogram_quantile(0.95, rate(execution_task_cost_bucket[1m]))",
            "refId": "B",
            "legendFormat": "p95"
          },
          {
            "expr": "histogram_quantile(0.99, rate(execution_task_cost_bucket[1m]))",
            "refId": "C",
            "legendFormat": "p99"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "fillOpacity": 15
            },
            "unit": "currencyUSD",
            "decimals": 4
          }
        }
      },
      {
        "id": 12,
        "title": "WASM Exit Codes",
        "type": "piechart",
        "gridPos": {"x": 12, "y": 22, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum by (exit_code) (execution_wasm_exit_codes_total)",
            "refId": "A",
            "legendFormat": "{{exit_code}}"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "values": ["value", "percent"]
          },
          "pieType": "donut"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "id": 13,
        "title": "Receipt Generation Rate",
        "type": "timeseries",
        "gridPos": {"x": 0, "y": 30, "w": 24, "h": 6},
        "targets": [
          {
            "expr": "rate(execution_receipts_total{status=\"signed\"}[1m])",
            "refId": "A",
            "legendFormat": "Signed"
          },
          {
            "expr": "rate(execution_receipts_total{status=\"verified\"}[1m])",
            "refId": "B",
            "legendFormat": "Verified"
          },
          {
            "expr": "rate(execution_receipts_total{status=\"failed\"}[1m])",
            "refId": "C",
            "legendFormat": "Failed"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "fillOpacity": 20
            },
            "unit": "receipts/s"
          }
        }
      }
    ]
  }
}
